    "core-js": "^3.37.0",
    "errorhandler": "^1.5.1",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "history": "^5.3.0",
    "http": "0.0.0",
    "isomorphic-fetch": "^3.0.0",
